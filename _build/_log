ocamlfind ocamlopt -c -package unix -package str -package extlib -I src -o src/main.cmx src/main.ml
# Target: src/main.native, tags: { package(extlib), package(str), package(unix), dont_link_with, extension:native, file:src/main.native, link, native, ocaml, program, quiet, traverse }
ocamlfind ocamlopt -linkpkg -package unix -package str -package extlib -I src src/main.cmx -o src/main.native
